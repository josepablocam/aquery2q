CREATE TABLE prices (
  Open          FLOAT,
  High          FLOAT,
  Low           FLOAT,
  Close         FLOAT,
  Volume        FLOAT,
  EndOfDayPrice FLOAT,
  ID            STRING,
  Date          DATE
)

LOAD DATA INFILE "./data/sp500_eod_complete_data.csv" INTO TABLE prices
FIELDS TERMINATED BY  ","

// Moving variance calculation for technology stocks
// AQuery vars built-in currently on another branch of git, but already
// implemented
<q>vars:{mavg[x;y*y]-m*m:mavg[x;y:"f"$y]}</q>
// Change folder to R/, since we want process to be running relative
// to the same folder as shiny app
<q>system "cd ./R/"</q>